{
	"name": "amediocredev",
	"version": "3.1.0",
	"private": true,
	"description": "aMediocreDev Portfolio Website",
	"homepage": "https://github.com/amediocredad/amediocredev",
	"repository": "amediocredad/amediocredev",
	"license": "MIT",
	"author": "Filip Ambrosius (https://github.com/amediocredad)",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"pages:deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
		"pages:dev": "wrangler pages dev --compatibility-date=2023-12-06 --proxy 5173 -- npm run dev",
		"preview": "vite preview",
		"start": "npm run dev -- --open"
	},
	"dependencies": {
		"@sanity/client": "6.9.3",
		"@sanity/image-url": "1.0.2",
		"@sveltejs/adapter-cloudflare": "2.3.4",
		"@sveltejs/kit": "1.27.7",
		"highlight.js": "11.9.0",
		"insane": "2.6.2",
		"marked": "11.0.1",
		"marked-highlight": "2.0.9",
		"svelte": "4.2.8",
		"svelte-preprocess": "5.1.1",
		"vite": "5.0.7"
	},
	"devDependencies": {
		"@babel/core": "7.23.5",
		"@types/marked": "4.3.0",
		"@typescript-eslint/eslint-plugin": "6.13.2",
		"@typescript-eslint/parser": "6.13.2",
		"autoprefixer": "10.4.16",
		"babel-loader": "9.1.3",
		"cssnano": "6.0.1",
		"eslint": "8.55.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"postcss": "8.4.32",
		"postcss-load-config": "5.0.2",
		"prettier": "~3.1.0",
		"prettier-plugin-svelte": "3.1.2",
		"sass": "1.69.5",
		"svelte-check": "3.6.2",
		"svelte-inview": "4.0.1",
		"typescript": "5.3.3",
		"vitest": "1.0.2",
		"wrangler": "3.19.0"
	}
}
